{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "css/style.css",
        "use": "@vercel/static"
      },
      {
        "src": "css/responsive.css",
        "use": "@vercel/static"
      },
      {
        "src": "bootstrap-5.2.3-dist/css/bootstrap.min.css",
        "use": "@vercel/static"
      },
      {
        "src": "bootstrap-5.2.3-dist/js/bootstrap.js",
        "use": "@vercel/static"
      },
      {
        "src": "images/favicon.png",
        "use": "@vercel/static"
      },
      {
        "src": "images/logo.jpg",
        "use": "@vercel/static"
      },
      {
        "src": "images/main-img.png",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/bootstrap-5.2.3-dist/css/(.*)",
        "dest": "/bootstrap-5.2.3-dist/css/$1"
      },
      {
        "src": "/bootstrap-5.2.3-dist/js/(.*)",
        "dest": "/bootstrap-5.2.3-dist/js/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/images/logo.jpg(.*)",
        "dest": "/images/logo.jpg$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  