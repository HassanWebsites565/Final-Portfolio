{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "css/responsive.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "Project-img/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "Project-1-Hospital/hospital.html",
      "use": "@vercel/static"
    },
    {
      "src": "Project-2-E-Commerce/commerce.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/Project-img/(.*)",
      "dest": "/Project-img/$1"
    },
    {
      "src": "/Project-1-Hospital/hospital.html",
      "dest": "/Project-1-Hospital/hospital.html"
    },
    {
      "src": "/Project-2-E-Commerce/commerce.html",
      "dest": "/Project-2-E-Commerce/commerce.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
